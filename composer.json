{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton of the Laravel application.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^3.0",
        "phpstan/phpstan": "^2.1",
        "psalm/plugin-laravel": "^3.0",
        "vimeo/psalm": "^6.13",
        "friendsofphp/php-cs-fixer": "^3.49",
        "squizlabs/php_codesniffer": "^3.8",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "analyse": [
            "@php vendor/bin/phpstan analyse --memory-limit=2G",
            "@php vendor/bin/psalm --no-progress"
        ],
        "pint": "@php vendor/bin/pint",
        "cs-check": "@php vendor/bin/phpcs --standard=PSR12 app/ config/ database/ routes/ tests/",
        "cs-fix": "@php vendor/bin/phpcbf --standard=PSR12 app/ config/ database/ routes/ tests/",
        "quality": [
            "@composer analyse",
            "@composer pint",
            "@composer cs-check",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
